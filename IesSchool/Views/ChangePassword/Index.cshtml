</head>

<body>

        <form id="frm" enctype="multipart/form-data">
    <input type="text" name="newPassword" id="newPassword" />
    <input type="text" name="confirmPassword" id="confirmPassword" />

    <input type="button" value="Save" onclick="SavePassword()" />
    <br />
    <p>Using TempData: @TempData.ContainsKey("msg")TempData["msg"].ToString() </p> 
            @*<label id="msg"  class="col-sm-3 col-form-label">@(TempData["msg"])</label>*@

    @*@if (ViewBag.Msg!= null)
    {
     <script>
         document.getElementById('msg').val()="@ViewBag.Msg";
     </script>
        
    }*@
</form>

</body>
</html>



<script type="text/javascript">
    

    function SavePassword () {


          $.ajax({
               type: "POST",
                url: "http://localhost:5212/api/ChangePassword/ResetUserPassword",
                data: { Id:50, NewPassword: $('#newPassword').val(), ConfirmPassword:  $('#confirmPassword').val() },
                dataType: "json",
                success: function (res) { //call successfull
                    console.log(res)
                },
                error: function (xhr) {
                }
            });
    }

</script>